{
  "name": "hangs-free",
  "main": "expo-router/entry",
  "version": "0.1.14",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --config jest.config.js --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "ts-check": "tsc-files --noEmit",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "release": "echo 'Releases are automated via Release Please. Push to main branch with conventional commits to trigger a release.'"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run ts-check"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "14.0.4",
