    "eslint-plugin-testing-library": "7.1.1",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-expo": "52.0.6",
    "lint-staged": "15.4.3",
    "prettier": "3.4.2",
    "react-test-renderer": "18.3.1",
    "tsc-files": "1.1.4",
    "typescript": "5.7.3"
  },
