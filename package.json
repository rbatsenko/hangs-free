    "expo-font": "13.0.4",
    "expo-haptics": "14.0.1",
    "expo-linking": "7.0.5",
    "expo-router": "~4.0.21",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "2.0.1",
    "expo-symbols": "0.2.2",
